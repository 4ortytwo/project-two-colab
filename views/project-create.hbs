<form action="/project-create" method="POST">
    <input type="text" name='projectName' placeholder="Name your project" autocomplete="off" required>
    <textarea type="text" name="description" placeholder="Describe your project"></textarea>
    <input type="text" name="skills" placeholder="Required skills">
    <input type="date" name="deadline" id="deadline">
    <button type="submit">Post the lab</button>
</form>